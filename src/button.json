        "medium": "$default",
        "normal": "$default",
        "m": "$default",
        "default": "$default",
        "small": "uk-button-small",
        "s": "uk-button-small",
        "sm": "uk-button-small",
        "large": "uk-button-large",
        "l": "uk-button-large",
        "lg": "uk-button-large"
    }
}
        "color",
        "size"
    ],
    "_default": {
        "base": "uk-button",
        "color": "uk-button-default",
        "size": ""
    },
    "color": {
        "": "$default",
        "default": "$default",
        "primary": "uk-button-primary",
        "secondary": "uk-button-secondary",
        "danger": "uk-button-danger",
        "text": "uk-button-text",
        "link": "uk-button-link"
    },
    "size": {
